<%= link_to 'Go Back to your Offers', recieved_offers_user_path(current_user) %>

<% if @offers.size > 0 %>
  <%= will_paginate @offers %>

  <% @offers.each do |offer| %>
    <%= render 'users/sent_offer', :offer => offer, :listing => offer.listing %>
  <% end %>

  <%= will_paginate @offers %>
<% end %>
